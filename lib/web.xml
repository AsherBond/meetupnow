<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <servlet>
        <servlet-name>MeetupNow</servlet-name>
        <servlet-class>meetupnow.MeetupNowServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MeetupNow</servlet-name>
        <url-pattern>/MeetupNow</url-pattern>
    </servlet-mapping>
    <servlet>
	<servlet-name>OAuth</servlet-name>
        <servlet-class>meetupnow.OAuthServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OAuth</servlet-name>
        <url-pattern>/oauth</url-pattern>
    </servlet-mapping>
    <servlet>
	<servlet-name>UserActions</servlet-name>
        <servlet-class>meetupnow.UserActions</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserActions</servlet-name>
        <url-pattern>/UserActions</url-pattern>
    </servlet-mapping>
    <servlet>
	<servlet-name>EventRegister</servlet-name>
        <servlet-class>meetupnow.EventRegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EventRegister</servlet-name>
        <url-pattern>/EventRegister</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

</web-app>
