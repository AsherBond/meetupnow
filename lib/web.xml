<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <servlet>
        <servlet-name>MeetupNow</servlet-name>
        <servlet-class>meetupnow.MeetupNowServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MeetupNow</servlet-name>
        <url-pattern>/MeetupNow</url-pattern>
    </servlet-mapping>
    <servlet>
	<servlet-name>OAuth</servlet-name>
        <servlet-class>meetupnow.OAuthServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OAuth</servlet-name>
        <url-pattern>/oauth</url-pattern>
    </servlet-mapping>
    <servlet>
	<servlet-name>Comment</servlet-name>
        <servlet-class>meetupnow.CommentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Comment</servlet-name>
        <url-pattern>/comment</url-pattern>
    </servlet-mapping>

    <servlet>
	<servlet-name>Logout</servlet-name>
        <servlet-class>meetupnow.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet>
	<servlet-name>UserActions</servlet-name>
        <servlet-class>meetupnow.UserActions</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserActions</servlet-name>
        <url-pattern>/UserActions</url-pattern>
    </servlet-mapping>
    <servlet>
	<servlet-name>EventRegister</servlet-name>
        <servlet-class>meetupnow.EventRegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EventRegister</servlet-name>
        <url-pattern>/EventRegister</url-pattern>
    </servlet-mapping>
    <servlet>
	<servlet-name>EventCreate</servlet-name>
        <servlet-class>meetupnow.EventCreateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EventCreate</servlet-name>
        <url-pattern>/EventCreate</url-pattern>
    </servlet-mapping>
    <servlet>
	<servlet-name>SetPrefs</servlet-name>
        <servlet-class>meetupnow.SetPrefsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SetPrefs</servlet-name>
        <url-pattern>/setprefs</url-pattern>
    </servlet-mapping>
    <servlet>
	<servlet-name>Event</servlet-name>
	<jsp-file>/EventPage.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
	<servlet-name>Event</servlet-name>
	<url-pattern>/Event</url-pattern>
    </servlet-mapping>
    <servlet>
	<servlet-name>Container</servlet-name>
	<jsp-file>/ContainerPage2.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
	<servlet-name>Container</servlet-name>
	<url-pattern>/Group</url-pattern>
    </servlet-mapping>
    <servlet>
	<servlet-name>TopicCreate</servlet-name>
        <servlet-class>meetupnow.TopicCreateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TopicCreate</servlet-name>
        <url-pattern>/TopicCreate</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

</web-app>
